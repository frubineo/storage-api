{
  "name": "bimstrength/storage-api",
  "description": "üìÅ Storage API - Ultra-High Performance Multi-Cloud Storage Engine",
  "type": "project",
  "license": "proprietary",
  "version": "2.0.0",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
